<%- include("partials/header") -%>


<h1>HOME</h1>
<p> <%= homeStarting %> </p>
<%  for(var i=0;i<posts.length;++i) { %>
<h3> <%=posts[i].entryTitle %></h3>
<p style="display:inline"> <%=posts[i].entryBody.substring(0,100) %> </p>
<form action="/more" method="post" style="display:inline">
<button class="more" style="display:inline"type="submit" name="button" value=<%=posts[i].entryTitle %>>more</button>
</form>



<% } %>




<%- include("partials/footer") -%>
